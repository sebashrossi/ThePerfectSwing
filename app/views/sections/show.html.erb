<div class="container justify-content-center text-center">
  <h1><%= @section.name %></h1>
  <div class="mt-5">
    <div>
      <iframe width="560" height="315" src="<%= @section.embedded_video_url %>" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
    </div>
  </div>

<div class="quiz-buttons mt-3">
  <%= link_to "Return to Trainings", training_path, class: "btn-secondary py-2 px-4 m-2" %>
  <%= link_to "Start Quiz", quiz_section_path(@section), class: "btn-primary py-2 px-4 m-2" %>
</div>

